+@COND_ANTLR_TRUE@%.tokens %.c %Lexer.c %Parser.c %Lexer.h %Parser.h %.h: %.g
+@COND_ANTLR_TRUE@	$(ANTLR) $(ANTLR_OPTIONS) $<
+
+@COND_ANTLR_TRUE@%.u: %.g
+@COND_ANTLR_TRUE@	$(ANTLR) -depend $< > $@
+@COND_ANTLR_TRUE@	@echo -n "ANTLR_PRODUCTS += " > $@.tmp
+@COND_ANTLR_TRUE@	@grep : $@ | cut -d : -f 1 | tr -d ' ' | { while read f; do test "$$f" != "$<" && echo -n "$$f "; done } >> $@.tmp
+@COND_ANTLR_TRUE@	@cat $@.tmp >> $@
+@COND_ANTLR_TRUE@	@rm $@.tmp
