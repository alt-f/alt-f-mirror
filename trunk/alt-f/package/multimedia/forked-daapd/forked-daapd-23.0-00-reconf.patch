+# gperf construction rules
+%_hash.c: %.gperf
+	if $(GPERF) $< > $@.tmp; then \
+		mv $@.tmp $@; \
+	elif $(GPERF) --version >/dev/null 2>&1; then \
+		rm $@.tmp; \
+		exit 1; \
+	else \
+		rm $@.tmp; \
+		touch $@; \
+	fi
